<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageVersion>0.1.0</PackageVersion>
    <Title>ATS.DarkSearch</Title>
    <Authors>ATS</Authors>
    <AssemblyVersion>0.1.0</AssemblyVersion>
    <FileVersion>0.1.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\links.txt" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Data\links.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <None Remove="Data\ServiceStackLicense.txt" />
    <Content Include="Data\ServiceStackLicense.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AngleSharp" Version="0.16.1" />
    <PackageReference Include="ATS-Common" Version="0.1.18" />
    <PackageReference Include="Knapcode.TorSharp" Version="2.8.1" />
    <PackageReference Include="NEST" Version="7.17.0" />
    <PackageReference Include="RabbitMQ.Client" Version="5.2.0" />
    <PackageReference Include="Serilog.AspNetCore" Version="4.1.0" />
    <PackageReference Include="ServiceStack.Api.OpenApi.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.OrmLite.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.OrmLite.Sqlite.Data" Version="6.0.2" />
    <PackageReference Include="ServiceStack.RabbitMq.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.Redis.Core" Version="6.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Logs" />
    <Folder Include="Reports\" />
  </ItemGroup>

</Project>
