<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageVersion>0.4.29</PackageVersion>
    <Title>ATS.DarkSearch</Title>
    <Authors>ATS</Authors>
    <AssemblyVersion>0.4.29</AssemblyVersion>
    <FileVersion>0.4.29</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\links.txt" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Data\links.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <None Remove="Data\ServiceStackLicense.txt" />
    <Content Include="Data\ServiceStackLicense.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <None Remove="Data\links_test.txt" />
    <Content Include="Data\links_test.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <None Remove="Data\links_ransomware.txt" />
    <Content Include="Data\links_ransomware.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AngleSharp" Version="0.16.1" />
    <PackageReference Include="ATS-Common" Version="0.2.28" />
    <PackageReference Include="NEST" Version="7.17.0" />
    <PackageReference Include="RabbitMQ.Client" Version="5.2.0" />
    <PackageReference Include="Serilog.AspNetCore" Version="5.0.0" />
    <PackageReference Include="ServiceStack.Api.OpenApi.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.Interfaces.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.OrmLite.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.OrmLite.Sqlite.Data" Version="6.0.2" />
    <PackageReference Include="ServiceStack.RabbitMq.Core" Version="6.0.2" />
    <PackageReference Include="ServiceStack.Redis.Core" Version="6.0.2" />
    <PackageReference Include="SharpCompress" Version="0.31.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Logs" />
  </ItemGroup>

</Project>
